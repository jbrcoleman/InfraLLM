# Outputs for S3 Bucket: {{ resource_name }}
# Generated by InfraLLM on {{ generated_at }}

output "bucket_id" {
  description = "The name of the bucket"
  value       = aws_s3_bucket.{{ tf_resource_name }}.id
}

output "bucket_arn" {
  description = "The ARN of the bucket"
  value       = aws_s3_bucket.{{ tf_resource_name }}.arn
}

output "bucket_domain_name" {
  description = "The bucket domain name"
  value       = aws_s3_bucket.{{ tf_resource_name }}.bucket_domain_name
}

output "bucket_regional_domain_name" {
  description = "The bucket region-specific domain name"
  value       = aws_s3_bucket.{{ tf_resource_name }}.bucket_regional_domain_name
}
